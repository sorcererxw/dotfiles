{
    "editor.fontSize": 18,
    "editor.fontFamily": "'sarasa mono sc', 'Fira Code Retina', Consolas, 'Courier New', monospace, 'source han sans'",
    "extensions.ignoreRecommendations": false,
    "files.autoSave": "afterDelay",
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC','sarasa ui sc', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "markdown.preview.fontSize": 18,
    "http.proxy": "http://127.0.0.1:1080",
    "editor.fontLigatures": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Default Material Dark Theme",
    "git.enableSmartCommit": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "window.zoomLevel": 0.5,
    "python.jediEnabled": false,
    "python.pythonPath": "~\\scoop\\shims\\python.exe",
    "python.linting.pylintEnabled": true
}